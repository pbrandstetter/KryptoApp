<Window x:Class="KryptoClient.Chat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KryptoClient"
        mc:Ignorable="d"
        Title="Chat" Height="450" Width="800">
    <Grid Height="440">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="0*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="TextEingabe" HorizontalAlignment="Left" Margin="10,357,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="615" Height="67"/>
        <Button x:Name="Send" Content="Send" HorizontalAlignment="Left" Margin="630,357,0,0" VerticalAlignment="Top" Height="67" Width="160" Click="Send_Click"/>
        <TextBlock x:Name="ChatHistory" Text="TextBlock" TextWrapping="Wrap" Margin="9,9,175,79"/>
        <Grid x:Name="AllUsers" HorizontalAlignment="Left" Height="320" Margin="630,32,0,0" VerticalAlignment="Top" Width="160"/>
        <Button x:Name="ReturnButton" Content="Return" HorizontalAlignment="Left" Height="25" Margin="632,2,0,0" VerticalAlignment="Top" Width="158" Click="ReturnButton_Click"/>
    </Grid>
</Window>
